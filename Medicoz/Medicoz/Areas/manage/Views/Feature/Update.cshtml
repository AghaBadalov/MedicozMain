
@{
    ViewData["Title"] = "Update";
}




@model Feature

<div class="col-sm-12 col-xl-6">
    <div class="bg-light rounded h-100 p-4">

        <form method="post" enctype="multipart/form-data">
            <div class="mb-3">
                <label asp-for="Tittle" class="form-label">Tittle</label>
                <input asp-for="Tittle" class="form-control">
                <span asp-validation-for="Tittle" class="text-danger"></span>

            </div>

            <div class="mb-3">
                <label asp-for="Desc" class="form-label">Desc</label>
                <input asp-for="Desc" class="form-control">
                <span asp-validation-for="Desc" class="text-danger"></span>

            </div>
            <div class="mb-3">
                <label asp-for="Icon" class="form-label">Icon</label>
                <input asp-for="Icon" class="form-control">
                <span asp-validation-for="Icon" class="text-danger"></span>

            </div>


            <button type="submit" class="btn btn-primary" class="text-danger">Update</button>
        </form>
    </div>
</div>




